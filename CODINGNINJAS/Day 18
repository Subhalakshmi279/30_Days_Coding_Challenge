sort 0 1 2
#include <bits/stdc++.h> 

void sort012(int *arr, int n)

{

   int s=0;

   int mid=0;

   int e=n-1;

   while(mid<=e){

      if(arr[mid]==0){

         swap(arr[s],arr[mid]);

         s++;

         mid++;

      }

      else if(arr[mid]==1){

         mid++;

      }

      else{

         swap(arr[mid],arr[e]);

         e--;

         

      }

   }

 

}
